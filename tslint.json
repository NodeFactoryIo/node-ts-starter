{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "no-namespace": false,
    "no-unused-expression": false,
    "interface-over-type-literal": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
